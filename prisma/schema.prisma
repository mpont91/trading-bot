// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model SpotEquity {
  id         Int      @id @default(autoincrement())
  amount     Decimal
  created_at DateTime @default(now())
}

model FuturesEquity {
  id         Int      @id @default(autoincrement())
  amount     Decimal
  created_at DateTime @default(now())
}
